@model JvHomes.Models.Home

@{
    Layout = "~/Views/Shared/WebsiteLayout.cshtml";
}

<div class="dt-responsive table-responsive">
    <table id="basic-btn" class="table table-striped table-bordered nowrap">
        <thead>
            <tr>
                <th>S No.</th>
                <th>Terms</th>


            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                if (Model.lstDetails != null)
                {
                    if (Model.lstDetails.Rows.Count > 0)
                    {
                        int j = 1;
                        for (int i = 0; i <= Model.lstDetails.Rows.Count - 1; i++)
                        {
                            <tr>
                                <td>@j</td>
                                <td>@Model.lstDetails.Rows[i]["Termscondition"].ToString()</td>
                                
                            </tr>
                            j++;
                        }

                    }
                }
            }
        </tbody>

    </table>
</div>