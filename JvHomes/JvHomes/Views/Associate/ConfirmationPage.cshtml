
@{
    ViewBag.Title = "ConfirmationPage";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<style type="text/css">
    .header-top {
        width: 100%;
        background-color: green;
    }

    ul.header-top-mail-number li {
        list-style-type: none;
        float: left;
        padding: 5px;
        font-size: 16px;
        color: aliceblue;
    }

    ul.header-top-login-inv li {
        list-style-type: none;
        float: left;
        padding: 5px;
        font-size: 16px;
        color: aliceblue;
        text-align: right;
    }

        ul.header-top-login-inv li a {
            color: aliceblue;
            line-height: 0px;
            margin: 10px;
            background-color: #27990c;
            padding: 10px;
            text-transform: uppercase;
            font-size: 13px;
            font-weight: bold;
            border-radius: 5px;
        }

    .icon-header-item li a {
        color: #fff;
    }

        .icon-header-item li a:hover {
            color: green;
        }
</style>
<style type="text/css">
    .header-top {
        width: 100%;
        background-color: green;
    }

    ul.header-top-mail-number li {
        list-style-type: none;
        float: left;
        padding: 5px;
        font-size: 16px;
        color: aliceblue;
    }

    ul.header-top-login-inv li {
        list-style-type: none;
