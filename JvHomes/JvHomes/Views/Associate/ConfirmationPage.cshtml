<div class="app-main">

    <div class="main-content">
        <div class="section-container">
            <h6 class="section-title">Confirmation Page</h6>

            <div class="section-wrapper white left">
                <section class="custom-padding about-us">
                    <div class="container">

                        <div class="row text-center" style="border:solid 1px">
                            <div class="col-md-12 form-group">
                                <br><br> <h2 style="color:#0fad00">Success</h2>

                                <img src="../../AdminPanelCss/img/XwHw.gif" style="height:150px" />
                                <h3>Dear, @Session["AssociateName"]</h3>
                                <p style="font-size:20px;color:#5C5C5C;text-align:center">
                                    Thank you for your registration. Now you are EASY TO EARN Business Associate.Your Details are as Below:
                                    <br />
                                    <span class="border-box">Login ID: @Session["AssociateLoginId"]</span>
                                    <span class="border-box">Password:  @Session["Password"]</span>
                                </p>
                                <a href="/Associate/DashBoard" class="btn btn-info">     Back    </a>

                                <br><br>
                            </div>
                        </div>

                    </div>
                </section>
            </div>
        </div>
    </div>
</div>


