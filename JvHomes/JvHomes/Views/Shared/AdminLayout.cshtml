    <script>
        $(document).ready(function () {
            $("#divload").css({ 'display': 'none' });
        });
        $('#txtid').keypress(function (event) {
            var loginid = $('#txtid').val();
            var keycode = (event.keyCode ? event.keyCode : event.which);
            if (keycode == '13') {
                window.location.href = "/Admin/GlobalSearch?text=" + loginid;
            }
        });
        var searchMenu = function () {
            var searchText = $("#filtersearch").val().toUpperCase();
            var $allListElements = $('.searchMenu > ul, .searchMenuLi > ul > li');

            var $matchingListElements = $allListElements.filter(function (i, li) {
                var listItemText = $(li).text().toUpperCase();
                return ~listItemText.indexOf(searchText);
            });
            $allListElements.hide();
            $matchingListElements.show();

            $(".searchMenuLi > li").show();

            if ($.trim($('#filtersearch').val()) == '') {
                $(".searchMenuLi > li").hide();
            }
        }
    </script>



